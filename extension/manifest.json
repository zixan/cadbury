{
    "manifest_version": 3,
    "name": "Cadbury",
    "version": "1.0",
    "description": "The agent that supercharges solutions consultants",
    "permissions": ["tabs", "runtime"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "externally_connectable": {
        "matches": ["<all_urls>"]
      }
  }